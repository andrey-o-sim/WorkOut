@{
    Layout = null;
}
<!DOCTYPE html>
<html ng-app="woApp">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/bundles/bootstrap")
    @Styles.Render("~/Content/css")
    @Styles.Render("~/app/assets/css")
    @Scripts.Render("~/app/assets/js")
    @Scripts.Render("~/app/components/modules")
    @Scripts.Render("~/app/components/services")
    @Scripts.Render("~/app/components/controllers")

    <!-- assets -->
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/app/assets/css/app.css" rel="stylesheet" />

    <script src="~/app/assets/libs/angular.js"></script>
    <script src="~/app/assets/libs/angular-route.js"></script>

    <!-- module -->
    <script src="~/app/app.module.js"></script>

    <!-- services -->
    <script src="~/app/shared/services/trainingType.service.js"></script>

    <!-- components -->
    <script src="~/app/components/TrainingType/forms/trainingType.new.controller.js"></script>
    <script src="~/app/components/TrainingType/forms/trainingType.edit.controller.js"></script>
    <script src="~/app/components/TrainingType/forms/trainingType.view.controller.js"></script>
    <script src="~/app/components/TrainingType/trainingType.route.js"></script>
</head>
<body>
    <div class="navbar navbar-inverse navbar-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><!--<a ng-href=''>Home</a>--></li>
                    <li><a href='#/trainingType/new'>Training Type</a></li>
                </ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        <ng-view></ng-view>
    </div>
</body>
</html>


